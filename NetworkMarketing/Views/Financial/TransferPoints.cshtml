
@{
    Layout = null;
}

<section class="content">
    <h2>Transfer Points</h2>

    <div class="row">
            <div class="col-lg-12">
                <div class="box box-primary">
                        <div class="box-header with-border">
                            <h3 class="box-title">Transfer Points</h3>
                        </div><!-- /.box-header -->
                        <div class="box-body">
                            <div class="form-group has-feedback">
                                <input type="password" class="form-control" placeholder="Transaction Code" name="password" required>
                                <span class="form-control-feedback"><i class="fa fa-lock"></i></span>
                            </div>
                        </div><!-- /.box-body -->
                        <div class="box-footer">
                            <button type="submit" onclick="location.href = '#/transfer' " class="btn btn-primary">
                                <i class="fa fa-floppy-o"></i>
                                &nbsp;Submit
                            </button>
                        </div><!-- box-footer -->
                </div><!-- /.box -->
            </div>
        </div>
</section>
